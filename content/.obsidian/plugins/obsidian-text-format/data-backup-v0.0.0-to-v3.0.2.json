{
  "MergeParagraph_Newlines": true,
  "MergeParagraph_Spaces": true,
  "LowercaseFirst": true,
  "RemoveBlanksWhenChinese": false,
  "ZoteroNoteRegExp": "‚Äú(?<text>.*)‚Äù \\((?<item>.*?)\\) \\(\\[pdf\\]\\((?<pdf_url>.*?)\\)\\)",
  "ZoteroNoteTemplate": "{text} [üîñ]({pdf_url})",
  "BulletPoints": "‚Ä¢‚Äì¬ßÔÅ∞",
  "WrapperList": [
    {
      "name": "Title",
      "prefix": "::: Title \\n",
      "suffix": "\\n :::"
    },
    {
      "name": "left-side",
      "prefix": "::: left-side \\n",
      "suffix": "\\n :::"
    },
    {
      "name": "right-side",
      "prefix": "::: right-side \\n",
